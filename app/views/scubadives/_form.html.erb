<%= simple_form_for(@scubadive) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :divespot %>
    <%= f.association :gear_set %>
    <%= f.input :enjoyment %>
    <%= f.input :comments %>
    <%= f.input :tip_for_others %>
    <%= f.input :map_tracks %>
    <%= f.input :visibility %>
    <%= f.input :start_air %>
    <%= f.input :end_air %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
