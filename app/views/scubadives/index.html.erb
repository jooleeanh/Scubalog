<p id="notice"><%= notice %></p>

<h1>Scubadives</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scubadives.each do |scubadive| %>
      <tr>
        <td><%= link_to 'Show', scubadive %></td>
        <td><%= link_to 'Edit', edit_scubadive_path(scubadive) %></td>
        <td><%= link_to 'Destroy', scubadive, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scubadive', new_scubadive_path %>
