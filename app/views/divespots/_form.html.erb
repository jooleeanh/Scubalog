<%= simple_form_for(@divespot) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :max_depth %>
    <%= f.input :avg_depth %>
    <%= f.input :salinity %>
    <%= f.input :shore %>
    <%= f.input :entry_difficulty %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
