<p id="notice"><%= notice %></p>

<h1>Freedive Sessions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @freedive_sessions.each do |freedive_session| %>
      <tr>
        <td><%= link_to 'Show', freedive_session %></td>
        <td><%= link_to 'Edit', edit_freedive_session_path(freedive_session) %></td>
        <td><%= link_to 'Destroy', freedive_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

